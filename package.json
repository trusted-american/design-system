{
  "name": "@trusted-american/design-system",
  "version": "0.0.231",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "typesVersions": {
    "*": {
      "test-support": [
        "declarations/addon-test-support/index.d.ts"
      ],
      "test-support/*": [
        "declarations/addon-test-support/*",
        "declarations/addon-test-support/*/index.d.ts"
      ],
      "*": [
        "declarations/addon/*",
        "declarations/addon/*/index.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "prepack": "tsc --project tsconfig.declarations.json",
    "postpack": "rimraf declarations",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@ember/render-modifiers": "^2.1.0",
    "@fortawesome/ember-fontawesome": "^1.0.3",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@github/markdown-toolbar-element": "^2.2.3",
    "@nullvoxpopuli/ember-composable-helpers": "^5.2.6",
    "bootstrap": "^5.3.3",
    "change-case": "^5.4.4",
    "dayjs": "^1.11.13",
    "ember-auto-import": "^2.10.0",
    "ember-basic-dropdown": "^8.4.0",
    "ember-breadcrumb-trail": "^1.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clipboard": "^1.2.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-showdown": "^9.0.1",
    "ember-concurrency": "^4.0.2",
    "ember-file-upload": "^9.1.0",
    "ember-modifier": "^4.2.0",
    "ember-pell": "^1.4.5",
    "ember-power-select": "^8.6.2",
    "ember-power-select-with-create": "^3.0.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-yeti-table": "^1.7.4",
    "identicon.js": "^2.3.3",
    "tracked-built-ins": "^3.4.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@popperjs/core": "^2.11.8",
    "@tsconfig/ember": "^3.0.8",
    "@types/bootstrap": "^5.2.10",
    "@types/identicon.js": "^2.3.5",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.0",
    "ember-cli": "~5.7.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^11.0.1",
    "ember-shiki": "^0.3.0",
    "ember-source": "~5.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.0.0",
    "ember-test-selectors": "^7.0.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.4.2",
    "qunit": "^2.23.1",
    "qunit-dom": "^3.4.0",
    "rimraf": "^6.0.1",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
