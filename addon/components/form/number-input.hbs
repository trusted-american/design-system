<Form::Input
  @type={{if @isRange "range" "number"}}
  @value={{this.value}}
  @label={{@label}}
  @identifier={{@identifier}}
  @isRequired={{true}}
  @help={{@help}}
  @invalidFeedback={{@invalidFeedback}}
  @size={{@size}}
  @isInputOnly={{@isInputOnly}}
  @errors={{@errors}}
  @onChange={{this.change}}
  ...attributes
/>